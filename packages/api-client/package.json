{
  "name": "@eco-base/api-client",
  "version": "1.0.0",
  "description": "Typed API + WebSocket client SDK for eco-base platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "generate": "openapi-typescript-codegen --input ../../backend/api/openapi.yaml --output src/generated --client fetch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/indestructibleorg/eco-base.git",
    "directory": "packages/api-client"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eco-base",
    "api-client",
    "sdk",
    "typescript",
    "websocket"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/indestructibleorg/eco-base/tree/main/packages/api-client",
  "bugs": {
    "url": "https://github.com/indestructibleorg/eco-base/issues"
  },
  "dependencies": {
    "socket.io-client": "~4.7.5"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "~0.30.0",
    "typescript": "~5.5.4"
  }
}
