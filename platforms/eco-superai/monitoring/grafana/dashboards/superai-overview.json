{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(http_requests_total{app=&quot;eco-api&quot;}[5m])",
          "legendFormat": "{{method}} {{path}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "title": "Response Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{app=&quot;eco-api&quot;}[5m]))",
          "legendFormat": "p95 {{path}}"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{app=&quot;eco-api&quot;}[5m]))",
          "legendFormat": "p99 {{path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 2 }
            ]
          }
        }
      }
    },
    {
      "title": "Error Rate (%)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "100 * sum(rate(http_requests_total{app=&quot;eco-api&quot;,status=~&quot;5..&quot;}[5m])) / sum(rate(http_requests_total{app=&quot;eco-api&quot;}[5m]))",
          "legendFormat": "Error %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "Active Connections",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "targets": [
        {
          "expr": "db_pool_active_connections{app=&quot;eco-api&quot;}",
          "legendFormat": "DB Pool"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 15 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      }
    },
    {
      "title": "Redis Cache Hit Ratio",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "100 * redis_cache_hits_total / (redis_cache_hits_total + redis_cache_misses_total)",
          "legendFormat": "Hit %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          }
        }
      }
    },
    {
      "title": "Quantum Job Duration",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(quantum_job_duration_seconds_bucket{app=&quot;eco-api&quot;}[5m]))",
          "legendFormat": "p95 {{algorithm}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "title": "CPU Usage by Pod",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{namespace=&quot;eco-base&quot;,container=&quot;api&quot;}[5m])",
          "legendFormat": "{{pod}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "title": "Memory Usage by Pod",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "container_memory_working_set_bytes{namespace=&quot;eco-base&quot;,container=&quot;api&quot;}",
          "legendFormat": "{{pod}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "palette-classic" }
        }
      }
    }
  ],
