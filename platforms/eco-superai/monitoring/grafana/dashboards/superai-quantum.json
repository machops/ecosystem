  "panels": [
    {
      "title": "Quantum Jobs Submitted (rate)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(eco-base_http_requests_total{endpoint=~&quot;/api/v1/quantum.*&quot;, method=&quot;POST&quot;}[5m]))",
          "legendFormat": "Jobs/s"
        }
