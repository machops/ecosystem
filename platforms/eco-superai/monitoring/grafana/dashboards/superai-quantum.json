      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "title": "Quantum Job Status Distribution",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(eco-base_http_requests_total{endpoint=~&quot;/api/v1/quantum.*&quot;}[1h])) by (status)",
          "legendFormat": "HTTP {{status}}"
        }
      ]
    },
    {
      "title": "Quantum Execution Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(eco-base_http_request_duration_seconds_bucket{endpoint=~&quot;/api/v1/quantum.*&quot;}[5m])) by (le))",
